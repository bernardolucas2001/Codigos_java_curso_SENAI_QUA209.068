
public class PrimeiroExercicio {

	public static void main(String[] args) {
		double Numero_1 = 50.25;
		double Numero_2 = 49.75; 
		
		String Nome = "Bernardo";
		String Sobrenome = "Lucas";
		
		
		
		
		
		System.out.println("A soma dos 2 números é igual a:" + Numero_1 + Numero_2);
		System.out.print("Meu nome completo é " + Nome + " " + Sobrenome);
		

	}

}
